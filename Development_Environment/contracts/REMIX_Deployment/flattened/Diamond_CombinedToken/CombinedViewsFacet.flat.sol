    function calculateGracePeriodExpiry(
        uint256 lastMissedPaymentTimestamp,
        uint16 gracePeriodDays
    ) internal pure returns (uint256) {
        return lastMissedPaymentTimestamp + (gracePeriodDays * 1 days);
    }

