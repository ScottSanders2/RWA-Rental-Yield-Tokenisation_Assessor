{
  "cli": {
    "version": ">= 5.9.0"
  },
  "build": {
    "detox": {
      "developmentClient": false,
      "distribution": "internal",
      "env": {
        "E2E_TEST_MODE": "1"
      },
      "ios": {
        "simulator": true,
        "buildConfiguration": "Release"
      }
    },
    "detox-debug": {
      "developmentClient": false,
      "distribution": "internal",
      "env": {
        "E2E_TEST_MODE": "1"
      },
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}


